{
  "manifest_version": 2,
  "author":"Vivek Shah",
  "name": "Random Quotes in New Tab",
  "description": "This extension shows a randome quotes on opening a new tab",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "incognito": "split",
  "permissions": [
    "activeTab"
  ],
  "icons":{
    "64":"icon1.png"
  },
  "chrome_url_overrides":{
    "newtab":"quotes.html"
  },
  "permissions": [
      "activeTab"
  ]
}
